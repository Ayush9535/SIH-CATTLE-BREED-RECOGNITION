  "breed_traits": {
    "Amritmahal": "Gray to steel-gray body with darker head and neck.",
    "Banni Buffalo": "Jet-black coat with straight backline and medium curved horns.",
    "Deoni": "White or grey body with irregular black patches on face or sides.",
    "Gir": "Convex dome-shaped forehead with long pendulous ears.",
    "HF Cross": "Black-and-white patch pattern resembling Holstein but irregular in crosses.",
    "Hallikar": "Light to dark grey coat with long straight horns curving outward.",
    "Jafarabadi Buffalo": "Massive head and large downward-curved horns forming a half-moon shape.",
    "Jersey Cross": "Light fawn to brown coat with a narrow face and darker eye patches.",
    "Kankrej": "Silver-grey coat with lyre-shaped horns pointing upward and outward.",
    "Khillar": "Whitish to light grey coat with strong muscular neck and forward-pointing horns.",
    "Kosali": "Reddish-brown compact body with small upward-pointing horns.",
    "Ladakhi": "Dark brown to black compact body with noticeably thick winter coat.",
    "Marathwadi Buffalo": "Black coat with moderately long backward-curving horns.",
    "Mehsana Buffalo": "Black coat with long flattened horns sweeping backward.",
    "Mewati": "Light grey to brown coat with straight medium-length horns.",
    "Murrah Buffalo": "Jet-black coat with very tightly curled small horns.",
    "Nari": "Light brown coat with straight upward-pointing horns and slender frame.",
    "Nili-Ravi Buffalo": "Black body with distinct white markings on forehead, tail switch, and legs.",
    "Ongole": "Large white body with prominent hump and long legs.",
    "Pandharpuri Buffalo": "Exceptionally long twisted or spiral horns extending sideways.",
    "Punganur": "Very small dwarf body with short legs and light brown coat.",
    "Purnea": "Brown coat with broad forehead and short upright horns.",
    "Rathi": "Light brown to white coat with small patches and short curving horns.",
    "Red Kandhari": "Uniform deep-red coat with medium straight horns.",
    "Red Sindhi": "Bright reddish coat with smooth skin and short curved horns.",
    "Sahiwal": "Reddish-brown smooth coat with loose skin and darker neck and shoulders.",
    "Tharparker": "White to light grey coat with darker neck and black tail switch.",
    "Thutho": "Dark brown or black coat with short straight horns and compact structure.",
    "Toda Buffalo": "Black coat with unique white facial markings around eyes and muzzle.",
    "Vechur": "Extremely small size with short legs and light brown coat."
  }
